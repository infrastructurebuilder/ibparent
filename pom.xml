            <failIfNoTests>${surefire.failIfNoTests}</failIfNoTests>
            <skipTests>false</skipTests>
            <trimStackTrace>false</trimStackTrace>
            <systemPropertyVariables>
              <target_dir>${project.build.directory}</target_dir>
